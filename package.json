{
  "name": "lance-game",
  "version": "2.0.0",
  "engines": {
    "node": ">= 7.6.0"
  },
  "description": "An HTML5 multiplayer game based on Lance",
  "main": "main.js",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "express": "^4.14.0",
